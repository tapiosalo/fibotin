{

  "manifest_version": 2,
  "name": "Fibotin",
  "version": "3.0",

  "permissions": [
    "activeTab",
    "contextMenus"
  ],

  "browser_action": {
    "default_icon": "data/icon-32.png",
    "default_title": "Fibotin",
    "default_popup": "data/panel.html"
  },

  "web_accessible_resources": [
    "data/style.css",
    "data/submit.js"
  ]
}     